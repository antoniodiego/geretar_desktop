<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{dd-MM-yyyy HH:mm:ss} %-5level %logger{36} %class{36} %L %M %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="Controller_Principal" level="TRACE"/>
        <Root level="error">
            <AppenderRef ref="Console" />
        </Root>
         <Logger name="dao_tarefas" level="TRACE"/>
    </Loggers>
</Configuration>
